{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-console": "off",
    "no-useless-escape": "off",
    "arrow-body-style": "off",
    "no-useless-computed-key": "off",
    "object-shorthand": "off",
    "prefer-template": "off",
    "no-debugger": "off",
    "no-unused-vars": "off"
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}